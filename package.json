{
  "name": "toggle-format-on",
  "displayName": "Toggle Format-On",
  "description": "Button for quickly toggling auto-formatting.",
  "keywords": [
    "toggle",
    "toggler",
    "button",
    "format",
    "format-on",
    "on",
    "keybind",
    "keybinder"
  ],
  "version": "0.0.1",
  "license": "GNU General Public License v3.0",
  "publisher": "iRewiewer",
  "engines": {
    "vscode": "^1.78.0"
  },
  "repository": "https://github.com/iRewiewer/FormatOnActivityBar",
  "bugs": {
    "url": "https://github.com/iRewiewer/FormatOnActivityBar/issues",
    "email": "irewiewer@gmail.com"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "toggle-format-on-container",
          "title": "Toggle format-on",
          "icon": "resources/toggle-format-on.svg"
        }
      ]
    },
    "views": {
      "toggle-format-on-container": [
        {
          "id": "toggle-format-on-view",
          "name": ""
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "toggle-format-on-view",
        "contents": "[Format on save](command:formatonactivitybar.formatonsave)\n[Format on paste](command:formatonactivitybar.formatonpaste)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}